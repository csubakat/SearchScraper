{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "SearchEngineProviderSettings": {
    "Google": {
      "BaseUrl": "https://www.google.com/",
      "SearchStringParameter": "search?q=",
      "NumberOfResultsParameter": "num="
    },
    "Yahoo": {
      "BaseUrl": "https://search.yahoo.com/",
      "SearchStringParameter": "search?p=",
      "NumberOfResultsParameter": "n="
    },
    "Bing": {
      "BaseUrl": "http://www.bing.com",
      "SearchStringParameter": "search?p=",
      "NumberOfResultsParameter": "count="
    }
  }
}
